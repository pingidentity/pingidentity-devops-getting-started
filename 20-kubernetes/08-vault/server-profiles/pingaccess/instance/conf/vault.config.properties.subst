#Vault Configuration properties
vault.api.address=$VAULT_ADDR
vault.api.token=VAULT_TOKEN
vault.api.verifyssl=true
vault.api.opentimeout=5
vault.api.readtimeout=5
vault.api.maxretry=3
vault.auth.roleid=VAULT_ROLE_ID
vault.auth.k8s.role=$K8S_AUTH_ROLE
vault.create.key=true
vault.secret.path=cleggett/pingfederate/dev/master
vault.transit.keyname.default=cleggett_dev_pingfederate_master
vault.transit.keytype=rsa-4096
vault.tls.server.
